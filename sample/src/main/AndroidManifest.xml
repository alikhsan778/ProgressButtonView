<?xml version="1.0" encoding="utf-8"?>
<manifest package="es.voghdev.progressbuttonview.sample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/progressButtonView_app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ViewWithWrongAttributesActivity" />
        <activity android:name=".ViewWithLowerCaseActivity" />
        <activity android:name=".ViewWithTwoBackgroundsActivity" />
        <activity android:name=".ImageButtonActivity" />
        <activity android:name=".CustomProgressBarActivity" />
        <activity android:name=".CustomProgressBarXMLActivity" />
    </application>

</manifest>